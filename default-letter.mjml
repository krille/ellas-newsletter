<mjml>
	<mj-head>
		<mj-attributes>
			<mj-all padding="0" margin="0" font-family="comic_ellamedium, 'Comic Sans MS', 'Comic Sans', sans-serif" />
		</mj-attributes>
		<mj-style>
			@font-face {
			font-family: "comic_ellamedium";
			font-style: normal;
			font-weight: 400;
			src: url("http://norrlandsgruppen.se/ellas-fonts/comicella-medium-webfont.eot?#iefix") format("embedded-opentype"),
			url("http://norrlandsgruppen.se/ellas-fonts/comicella-medium-webfont.woff2") format("woff2"),
			url("http://norrlandsgruppen.se/ellas-fonts/comicella-medium-webfont.woff") format("woff"),
			url("http://norrlandsgruppen.se/ellas-fonts/comicella-medium-webfont.ttf") format("truetype"),
			url("http://norrlandsgruppen.se/ellas-fonts/comicella-medium-webfont.svg#comic_ellamedium") format("svg");
			mso-font-alt:'Comic Sans MS';
			}

			@font-face {
			font-family: "ellas_kitchen_1regular";
			font-style: normal;
			font-weight: 400;
			src: url("http://norrlandsgruppen.se/ellas-fonts/ellaskitchen1.eot?#iefix") format("embedded-opentype"),
			url("http://norrlandsgruppen.se/ellas-fonts/ellaskitchen1.woff2") format("woff2"),
			url("http://norrlandsgruppen.se/ellas-fonts/ellaskitchen1.woff") format("woff"),
			url("http://norrlandsgruppen.se/ellas-fonts/ellaskitchen1.ttf") format("truetype"),
			url("http://norrlandsgruppen.se/ellas-fonts/ellaskitchen1.svg#ellas_kitchen_1regular") format("svg");
			mso-font-alt:'Comic Sans MS';
			}

			td {
				padding: 0;
			}

			p {
				margin: 0 0 1em;
			}

			h1, h2, h3 {
				font-weight: normal;
				margin: 0;
			}

			img {
				border-radius: 0 !important;
			}

			@media only screen and (max-width: 750px) {
				h1,
				h2,
				h3,
				h4,
				p,
				span,
				div *,
				table *,
				tr *,
				td * {
					background: transparent !important;
				}
			}

			@media only screen and (max-width: 479px) {
				.mobile-text-center {
					text-align: center;
				}
				.mobile-img-center {
					margin-left: auto;
					margin-right: auto;
				}
			}
		</mj-style>
		<mj-style inline="inline">
			.text-center {
				text-align: center;
			}
			.text-brown {
				color: #6c3515 !important;
			}
			.d-block {
				display: block;
			}
			.img-fluid {
				max-width: 100% !important;
				height: auto !important;
			}
			.heading-orange {
				color: #f07037;
				font-size: 18px;
				line-height: 28px;
			}
			.text-14 {
				color: #ffffff;
				font-size: 14px;
				line-height: 21px;
			}
			.text-14-blue {
				color: #253068;
				font-size: 14px;
				line-height: 21px;
			}
			.text-16 {
				color: #53160d;
				font-size: 16px;
				line-height: 24px;
			}
			.heading-24 {
				color: #00b450;
				font-family: 'ellas_kitchen_1regular', 'Comic Sans MS', 'Comic Sans', sans-serif;
				font-size: 24px;
				line-height: 24px;
				letter-spacing: -0.05em;
				font-weight: normal;
			}
			.heading-36 {
				color: #fbd740;
				font-family: 'ellas_kitchen_1regular', 'Comic Sans MS', 'Comic Sans', sans-serif;
				font-size: 36px;
				line-height: 40px;
				letter-spacing: -0.05em;
				font-weight: normal;
			}
			.heading-76 {
				color: #fbd740;
				font-family: 'ellas_kitchen_1regular', 'Comic Sans MS', 'Comic Sans', sans-serif;
				font-size: 70px;
				line-height: 40px;
				letter-spacing: -0.1em;
				font-weight: normal;
			}
			.webversion-link {
				color: #939598 !important;
				font-family: 'comic_ellamedium', 'Comic Sans MS', 'Comic Sans', sans-serif;
				font-size: 14px;
				line-height: 22px;
				text-decoration: underline !important;
			}
			.unsubscribe-link {
				color: #ffffff !important;
				font-family: 'comic_ellamedium', 'Comic Sans MS', 'Comic Sans', sans-serif;
				text-decoration: underline !important;
			}
		</mj-style>
	</mj-head>
	<mj-body>
